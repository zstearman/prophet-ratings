<div id="homeCarousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#homeCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#homeCarousel" data-slide-to="1"></li>
    <li data-target="#homeCarousel" data-slide-to="2"></li>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src="https://cdn-s3.si.com/s3fs-public/styles/marquee_large_2x/public/2017/10/02/college-basketball-recruiting-scandal-sonny-vaccaro.jpg" alt="basketballs">
      <div class="carousel-caption">
        <h1>Predictive Analytics</h1>
        <p>This is placeholder info for when we fill in the home page.</p>
      </div>
    </div>

    <div class="item">
      <img src="https://cdn-s3.si.com/s3fs-public/2017/09/21/ncaa-transfer-rules-basketball-changes.jpg" alt="Chicago">
      <div class="carousel-caption">
        <h1>Chicago</h1>
        <p>Thank you, Chicago!</p>
      </div>
    </div>

    <div class="item">
      <img src="https://cdn.vox-cdn.com/thumbor/J_R7SzvpNxrGRYEqHPjTw5GC5P8=/0x0:4000x2667/1200x800/filters:focal(0x0:4000x2667)/cdn.vox-cdn.com/uploads/chorus_image/image/31295281/483295597.0.jpg" alt="New York">
      <div class="carousel-caption">
        <h1>New York</h1>
        <p>We love the Big Apple!</p>
      </div>
    </div>
  </div>

  <!-- Left and right controls -->
  <a class="left carousel-control" href="#homeCarousel" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#homeCarousel" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
<div>
    <h2>Pricing Structures</h2>
</div>
<%= render 'layouts/objects/carousel' %>