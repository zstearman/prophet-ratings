<div class="page-content logged-out-body">
  <div id="homeCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#homeCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#homeCarousel" data-slide-to="1"></li>
      <li data-target="#homeCarousel" data-slide-to="2"></li>
    </ol>
  
    <!-- Wrapper for slides -->
    <div class="carousel-inner">
      <div class="item active">
        <img src="https://cdn-s3.si.com/s3fs-public/styles/marquee_large_2x/public/2017/10/02/college-basketball-recruiting-scandal-sonny-vaccaro.jpg" alt="basketballs">
        <div class="carousel-caption">
          <h1>Predictive Analytics</h1>
          <p>This is placeholder info for when we fill in the home page.</p>
        </div>
      </div>
  
      <div class="item">
        <img src="https://cdn-s3.si.com/s3fs-public/2017/09/21/ncaa-transfer-rules-basketball-changes.jpg" alt="Tourney">
        <div class="carousel-caption">
          <h1>Betting Advice</h1>
          <p>More cool features!</p>
        </div>
      </div>
  
      <div class="item">
        <img src="https://cdn-s3.si.com/s3fs-public/styles/marquee_large_2x/public/2017/12/04/villanova-virginia-ncaa-basketball-undefeated-teams-jimmy-v-classic.jpg" alt="Villanova">
        <div class="carousel-caption">
          <h1>Tournament Simulator</h1>
          <p>Win your bracket pool!</p>
        </div>
      </div>
    </div>
  
    <!-- Left and right controls -->
    <a class="left carousel-control" href="#homeCarousel" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#homeCarousel" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
  <%= render 'layouts/objects/plans' %>
  <%= render 'layouts/objects/carousel' %>
</div>